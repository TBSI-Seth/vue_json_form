{
  "name": "@educorvi/vue-json-form",
  "version": "0.3.2",
  "private": false,
  "main": "dist/vue-json-form.js",
  "files": [
    "dist/*",
    "src/components/*"
  ],
  "author": "Julian Pollinger",
  "repository": "https://github.com/educorvi/vue_json_form.git",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:npm && npm run build:demo",
    "build:npm": "rimraf ./dist && webpack --config ./webpack.config.js",
    "build:demo": "vue-cli-service build --dest demoDist",
    "lint": "vue-cli-service lint",
    "doc": "vuese gen && copyfiles README.md docs -E && copyfiles ./src/schemas/uischema.json docs -E -f && git add docs",
    "zip": "npm run zip:npm && npm run zip:demo",
    "zip:npm": "node zip.js dist",
    "zip:demo": "node zip.js demoDist --output demoDist.zip",
    "release": "dotenv release-it",
    "clean": "del-cli dist* demoDist*"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "json-pointer": "^0.6.1",
    "jsonschema": "^1.2.7",
    "vue": "^2.6.12",
    "vuedraggable": "^2.24.1"
  },
  "devDependencies": {
    "@release-it/bumper": "^2.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vuese/cli": "^2.14.0",
    "archiver": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "bootswatch": "^4.5.2",
    "copyfiles": "^2.3.0",
    "css-loader": "^5.0.0",
    "del-cli": "^3.0.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "release-it": "^14.1.0",
    "sass-loader": "^10.0.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "vuese": {
    "include": [
      "**/*.vue",
      "**/*.js"
    ],
    "outDir": "docs",
    "title": "Vue JSON Form"
  }
}
