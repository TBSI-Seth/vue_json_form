{
  "name": "vue_json_form",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "doc": "vuese gen && copyfiles README.md docs -E && copyfiles ./src/schemas/uischema.json docs -E -f && git add docs"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "json-pointer": "^0.6.1",
    "jsonschema": "^1.2.7",
    "vue": "^2.6.12",
    "vuedraggable": "^2.24.1"
  },
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vuese/cli": "^2.14.0",
    "babel-eslint": "^10.1.0",
    "bootswatch": "^4.5.2",
    "copyfiles": "^2.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "vuese": {
    "include": [
      "**/*.vue",
      "**/*.js"
    ],
    "outDir": "docs",
    "title": "Vue JSON Form"
  }
}
